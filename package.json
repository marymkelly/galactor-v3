	"scripts": {
		"dev": "concurrently \"npm:webpack-build\" \"npm:css \" \"npm:server\" \"npm:webpack-dev\"",
		"build": "npm run css-build && npm run webpack-build",
		"webpack-dev": "npx webpack serve",
		"webpack-build": "Webpack .",
		"css-build": "npx tailwindcss -i ./src/styles/global.css -o ./public/dist/build.css",
		"css": "npx tailwindcss -i ./src/styles/global.css -o ./public/dist/build.css --watch",
		"server": "nodemon -r dotenv/config src/server.js dotenv_config_path=./config/.env"
	},
